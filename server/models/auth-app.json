{
  "name": "AuthApp",
  "base": "Application",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Stamp": {}
  },
  "properties": {
    "authUrl": "string"
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "oauthCallback"
    }
  ],
  "methods": {}
}
