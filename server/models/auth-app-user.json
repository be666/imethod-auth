{
  "name": "AuthAppUser",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Stamp": {}
  },
  "properties": {
    "userId": {
      "type": "String",
      "required": true
    },
    "appId": {
      "type": "String",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "authLogin"
    }
  ],
  "methods": {}
}
