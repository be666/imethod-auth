{
  "name": "AuthUser",
  "base": "User",
  "idInjection": true,
  "properties": {
    "email": {
      "type": "string"
    },
    "mobile": {
      "type": "String",
      "required": false,
      "length": 100,
      "precision": null,
      "scale": null,
      "_selectable": true
    },
    "userType": {
      "type": "number",
      "required": false,
      "length": 2,
      "precision": null,
      "scale": null,
      "_selectable": true
    },
    "openId": {
      "type": "string",
      "required": false,
      "length": 100,
      "precision": null,
      "scale": null,
      "_selectable": true
    }
  },
  "options": {
    "caseSensitiveEmail": false
  },
  "validations": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "*",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "EXECUTE",
      "permission": "ALLOW",
      "property": "info"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "EXECUTE",
      "permission": "ALLOW",
      "property": "webLogin"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "EXECUTE",
      "permission": "ALLOW",
      "property": "webSign"
    }
  ],
  "relations": {
    "userApp": {
      "type": "hasMany",
      "model": "AuthApp",
      "foreignKey": "userId"
    },
    "userGroup": {
      "type": "hasMany",
      "model": "AuthGroup",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "Stamp": {}
  }
}
